{
  "name": "overdub",
  "version": "0.0.0",
  "description": "Add full ES.Next support to Node.js",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/overdub",
  "bugs": "https://github.com/vinsonchuong/overdub/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/overdub",
  "scripts": {
    "prepare": "flow-typed update --skip",
    "test": "standard -v --fix && flow && ava -v",
    "release": "semantic-release",
    "prepack": "build-esm"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-preset-diff": "^6.2.0"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "build-esm": "^4.2.1",
    "flow-bin": "^0.104.0",
    "flow-typed": "^2.6.1",
    "semantic-release": "^15.13.19",
    "standard-esnext": "^5.3.3",
    "tempy": "^0.3.0"
  },
  "babel": {
    "presets": [
      "diff"
    ]
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "helpers": [
      "test/fixtures/**/*",
      "test/helpers/**/*"
    ]
  }
}
